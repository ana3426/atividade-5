def fatorial(n):
    if n < 0:
        return "Erro: Fatorial não definido para números negativos"
    if n == 0 or n == 1:
        return 1
    
    resultado = 1
    for i in range(2, n + 1):
        resultado *= i
    return resultado

numeros_impares = [1, 3, 5, 7, 9]

for numero in numeros_impares:
    resultado_fatorial = fatorial(numero)
    print(f"{numero}!={resultado_fatorial}")
